{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "lib2": {
      "root": "lib",
      "sourceRoot": "lib/src",
      "projectType": "library",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-ng-packagr:build",
          "options": {
            "tsConfig": "lib/tsconfig.lib.json",
            "project": "lib/ng-package.json"
          }
        }
      }
    }
  },
  "defaultProject": "lib2"
}